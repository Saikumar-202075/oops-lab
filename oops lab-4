
#include <iostream>

using namespace std;
class Overloading
{public:
    int power(int n,int m)
    {
        int i,result=1;
        for(i=0;i<m;i++)
        {
            result=result*n;
        }
        return result;
    }
    int power(int n)
    {
        int m=2;
        int i,result=1;
        for(i=0;i<m;i++)
        {
            result=result*n;
        }
        return result;
    }
};

int main()
{
    Overloading x;
    cout<<x.power(3,4);

    return 0;
}
